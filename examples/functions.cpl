start
    int c = 24;
    int g = 12;
    
    function sumfunc int a int b; fstart
    fend a + b;

    function divfunc int a int b; fstart
    fend a - b;

    int ffuncres = callfunc sumfunc g c; : 24 + 12 = 36 :
    int sfuncres = callfunc divfunc g c; : 24 - 12 = 12 :
exit ffuncres + sfuncres; : 36 + 12 = 48 :