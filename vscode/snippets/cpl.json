{
  "function-definition": {
    "scope": "cpl",
    "prefix": "function",
    "body": [
      "function ${1:name}(${2:})${3: -> ${4:i32}} {",
      "\t${0:return 0;}",
      "}"
    ],
    "description": "Function definition (return type optional)"
  },

  "function-prototype": {
    "scope": "cpl",
    "prefix": "fproto",
    "body": [
      "function ${1:name}(${2:})${3: -> ${4:i32}};"
    ],
    "description": "Function prototype (return type optional)"
  },

  "import-op": {
    "scope": "cpl",
    "prefix": "from",
    "body": [
      "from \"${1:module}\" import ${0:name}"
    ],
    "description": "Import statement"
  },

  "extern-function": {
    "scope": "cpl",
    "prefix": "externfn",
    "body": [
      "extern function ${1:name}(${2:})${3: -> ${4:i32}};"
    ],
    "description": "Extern function prototype"
  },

  "extern-var": {
    "scope": "cpl",
    "prefix": "externvar",
    "body": [
      "extern ${1:i32} ${0:name}"
    ],
    "description": "Extern variable prototype"
  },

  "section-block": {
    "scope": "cpl",
    "prefix": "section",
    "body": [
      "section(\"${1:.text}\") {",
      "\t${0}",
      "}"
    ],
    "description": "Top-level section block"
  },

  "align-declaration": {
    "scope": "cpl",
    "prefix": "align",
    "body": [
      "align(${1:16}) ${2:i32 ${3:name}${4: = ${5:0}};}"
    ],
    "description": "Aligned declaration"
  },

  "align-block": {
    "scope": "cpl",
    "prefix": "alignblock",
    "body": [
      "align(${1:16}) {",
      "\t${0:i32 x = 0;}",
      "}"
    ],
    "description": "Aligned declaration block"
  },

  "poparg-assignment": {
    "scope": "cpl",
    "prefix": "popargvar",
    "body": [
      "${1:i32} ${2:name} = poparg;"
    ],
    "description": "Assign poparg to local variable"
  },

  "CPL Ops POPARG": {
    "scope": "cpl",
    "prefix": "poparg",
    "body": "poparg",
    "description": "CPL poparg expression"
  },

  "CPL Types void": {
    "scope": "cpl",
    "prefix": "i0",
    "body": "i0",
    "description": "CPL i0 type"
  },

  "CPL Keywords Section": {
    "scope": "cpl",
    "prefix": "section",
    "body": "section",
    "description": "CPL keyword"
  },

  "CPL Keywords Align": {
    "scope": "cpl",
    "prefix": "align",
    "body": "align",
    "description": "CPL keyword"
  }
}