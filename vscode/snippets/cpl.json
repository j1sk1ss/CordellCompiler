{
  "program-start": {
    "scope": "cpl",
    "prefix": "start",
    "body": [
      "{",
      "\tstart(i64 argc, ptr u64 argv) {",
      "\t\t$0",
      "\t\texit ${1:0};",
      "\t}",
      "}"
    ],
    "description": "CPL program skeleton"
  },

  "asm-block": {
    "scope": "cpl",
    "prefix": "asm",
    "body": [
      "asm (${1:arg1}${2:, arg2}) {",
      "\t\"${0:mov rax, &arg1}\"",
      "}"
    ],
    "description": "Inline asm block"
  },

  "function-definition": {
    "scope": "cpl",
    "prefix": "function",
    "body": [
      "function ${1:name}(${2:}) -> ${3:i32} {",
      "\t${0:return 0;}",
      "}"
    ],
    "description": "Function definition"
  },

  "function-prototype": {
    "scope": "cpl",
    "prefix": "fproto",
    "body": [
      "function ${1:name}(${2:})${3: -> ${4:i32}};"
    ],
    "description": "Function prototype (declaration)"
  },

  "global-var": {
    "scope": "cpl",
    "prefix": "globvar",
    "body": [
      "glob ${1:i32} ${2:name}${3: = ${4:0}};"
    ],
    "description": "Global variable declaration"
  },

  "ro-global-var": {
    "scope": "cpl",
    "prefix": "rovar",
    "body": [
      "ro ${1:i32} ${2:name}${3: = ${4:0}};"
    ],
    "description": "Read-only global variable declaration"
  },

  "local-var": {
    "scope": "cpl",
    "prefix": "var",
    "body": [
      "${1:i32} ${2:name}${3: = ${4:0}};"
    ],
    "description": "Local variable declaration"
  },

  "array-declaration": {
    "scope": "cpl",
    "prefix": "arr",
    "body": [
      "arr ${1:name}[${2:10}, ${3:i32}]${4: = {${5:0}}};"
    ],
    "description": "Array declaration"
  },

  "if-statement": {
    "scope": "cpl",
    "prefix": "if",
    "body": [
      "if ${1:cond}; {",
      "\t${0:: ... :}",
      "}",
      "else {",
      "\t: ... :",
      "}"
    ],
    "description": "If / else statement"
  },

  "while-loop": {
    "scope": "cpl",
    "prefix": "while",
    "body": [
      "while ${1:cond}; {",
      "\t${0:: ... :}",
      "}"
    ],
    "description": "While loop"
  },

  "loop-loop": {
    "scope": "cpl",
    "prefix": "loop",
    "body": [
      "loop {",
      "\t${0:: ... :}",
      "}"
    ],
    "description": "Infinite loop"
  },

  "switch-statement": {
    "scope": "cpl",
    "prefix": "switch",
    "body": [
      "switch ${1:expr}; {",
      "\tcase ${2:0}; {",
      "\t\t${0:: ... :}",
      "\t}",
      "\tdefault {",
      "\t\t: ... :",
      "\t}",
      "}"
    ],
    "description": "Switch statement (switch expr; { case lit; block })"
  },

  "return-statement": {
    "scope": "cpl",
    "prefix": "return",
    "body": [
      "return ${0};"
    ],
    "description": "Return statement"
  },

  "break-statement": {
    "scope": "cpl",
    "prefix": "break",
    "body": [
      "break;"
    ],
    "description": "Break statement"
  },

  "exit-statement": {
    "scope": "cpl",
    "prefix": "exit",
    "body": [
      "exit ${0:0};"
    ],
    "description": "Exit statement"
  },

  "lis-statement": {
    "scope": "cpl",
    "prefix": "lis",
    "body": [
      "lis \"${0:text}\";"
    ],
    "description": "LiS statement"
  },

  "syscall-invoke": {
    "scope": "cpl",
    "prefix": "syscall",
    "body": [
      "syscall(${1:arg1}${2:, arg2});"
    ],
    "description": "Syscall invocation"
  },

  "import-op": {
    "scope": "cpl",
    "prefix": "from",
    "body": [
      "from \"${1:module}\" import ${0:name};"
    ],
    "description": "Import statement"
  },

  "extern-exfunc": {
    "scope": "cpl",
    "prefix": "externex",
    "body": [
      "extern exfunc ${0:name};"
    ],
    "description": "Extern function prototype"
  },

  "extern-var": {
    "scope": "cpl",
    "prefix": "externvar",
    "body": [
      "extern ${1:i32} ${0:name};"
    ],
    "description": "Extern variable prototype"
  },

  "pp-include": {
    "scope": "cpl",
    "prefix": "#include",
    "body": [
      "#include \"${0:file}\""
    ],
    "description": "Preprocessor include directive"
  },

  "pp-define": {
    "scope": "cpl",
    "prefix": "#define",
    "body": [
      "#define ${1:NAME} ${0:value}"
    ],
    "description": "Preprocessor define directive"
  },

  "pp-undef": {
    "scope": "cpl",
    "prefix": "#undef",
    "body": [
      "#undef ${0:NAME}"
    ],
    "description": "Preprocessor undef directive"
  },

  "pp-ifdef": {
    "scope": "cpl",
    "prefix": "#ifdef",
    "body": [
      "#ifdef ${1:NAME}",
      "${0}",
      "#endif"
    ],
    "description": "Preprocessor ifdef block"
  },

  "pp-ifndef": {
    "scope": "cpl",
    "prefix": "#ifndef",
    "body": [
      "#ifndef ${1:NAME}",
      "${0}",
      "#endif"
    ],
    "description": "Preprocessor ifndef block"
  },

  "pp-line": {
    "scope": "cpl",
    "prefix": "#line",
    "body": [
      "#line ${1:1} \"${0:file}\""
    ],
    "description": "Preprocessor line directive"
  },

  "pp-endif": {
    "scope": "cpl",
    "prefix": "#endif",
    "body": [
      "#endif"
    ],
    "description": "Preprocessor endif directive"
  },

